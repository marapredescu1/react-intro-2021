{
    "extends": [
      "airbnb"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0,
      "react/button-has-type": 0,
      "react/jsx-props-no-spreading": 0,
      "object-curly-newline": 1,
      "arrow-body-style": 0,
      "no-unused-vars": 0
    }
}
